generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/@prisma/postgres/client"
}

datasource db {
  provider = "postgresql"
  url      = env("SQL_DATABASE_URL")
}

model Test {
  id   String @id @default(uuid())
  name String
}
